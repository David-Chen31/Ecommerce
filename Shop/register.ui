    background-color: #E6F7FF; /* 浅天蓝背景 */
    padding: 10px;
    text-align: center;
}

/* 欢迎标题样式 */
QLabel#WelcomeLabel {
    font-size: 18px;
    font-weight: bold;
    color: #333333; /* 深灰色字体 */
}

/* 输入框样式 */
QLineEdit {
    border: 1px solid #CCCCCC; /* 浅灰边框 */
    border-radius: 5px;
    padding: 6px;
    font-size: 16px;
    background-color: #F7FAFD; /* 浅白蓝背景 */
}

QLineEdit:focus {
    border: 2px solid #0099FF; /* 聚焦天蓝边框 */
}

/* 按钮样式 */
QPushButton {
    background-color: #0099FF; /* 天蓝色按钮背景 */
    color: white; /* 白色文字 */
    border-radius: 5px;
    padding: 8px 16px;
    font-size: 16px;
    font-weight: bold;
}

QPushButton:hover {
    background-color: #007ACC; /* 深蓝色悬停背景 */
}

QPushButton:pressed {
    background-color: #005F99; /* 深蓝色按下背景 */
}

/* 返回按钮 */
QPushButton#GoBack {
    background-color: transparent; /* 透明背景 */
    color: #0099FF; /* 天蓝色文字 */
    font-size: 14px;
    border: none;
    text-decoration: underline;
}

QPushButton#GoBack:hover {
    color: #005F99; /* 深蓝色悬停 */
}

/* 表单标签样式 */
QLabel {
    font-size: 16px;
    color: #333333; /* 深灰色字体 */
}

/* 用户协议复选框样式 */
QRadioButton#AgreementRadioButton {
    font-size: 14px;
    color: #555555; /* 中灰色字体 */
}

QRadioButton::indicator {
    width: 16px;
    height: 16px;
}

QRadioButton::indicator:checked {
    background-color: #0099FF; /* 天蓝色选中背景 */
    border: 1px solid #0099FF; /* 天蓝色边框 */
}

/* 版本号样式 */
QLabel#VersionLabel {
    font-size: 12px;
    color: #AAAAAA; /* 浅灰色字体 */
}
</string>
