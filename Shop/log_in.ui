    font-size: 14px;
    color: #555555; /* 中灰色字体 */
}

QRadioButton::indicator {
    width: 16px;
    height: 16px;
}

QRadioButton::indicator:checked {
    background-color: #0099FF; /* 天蓝色选中背景 */
    border: 1px solid #0099FF; /* 天蓝色边框 */
}
