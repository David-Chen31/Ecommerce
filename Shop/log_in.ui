    padding: 8px 16px;
    font-size: 16px;
    font-weight: bold;
}

QPushButton:hover {
    background-color: #007ACC; /* 深蓝色悬停 */
}

QPushButton:pressed {
    background-color: #005F99; /* 更深蓝色按下效果 */
}

