QLabel#SelectedNumber, QLabel#Total {
    font-size: 20px;
    color: #555555;
}

QLabel#selectedValue, QLabel#TotalValue {
    font-size: 20px;
    color: #0078D7;
    font-weight: bold;
}

/* 按钮样式 */
QPushButton#Settle {
    background-color: #0078D7;
    color: white;
    border-radius: 8px;
    padding: 10px;
    font-weight: bold;
}

QPushButton#Settle:hover {
    background-color: #005EA6;
    border: 1px solid #004A8C;
}

QPushButton#Settle:pressed {
    background-color: #004A8C;
}

/* 全选按钮样式 */
QRadioButton#checkAll {
    font-size: 20px;
    color: #555555;
}

/* 鼠标悬停效果 */
QLabel:hover {
    color: #0078D7;
    text-decoration: underline;
}
