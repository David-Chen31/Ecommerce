}

/* 滚动区域样式 */
QScrollArea {
    border: 1px solid #dddddd;
    background-color: #ffffff;
    border-radius: 8px;
}

QScrollArea QWidget {
    background-color: #ffffff;
}

/* 图片 LOGO 样式 */
QLabel#clientpingCartLogo {
    border: 1px solid #dddddd;
    border-radius: 8px;
    margin: 10px;
}

